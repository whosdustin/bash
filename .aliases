## get rid of command not found ##
alias cd..='cd ..'

## a quick way to get out of current directory ##
alias ..='cd ..'
alias ...='cd ../../../'
alias ....='cd ../../../../'
alias .....='cd ../../../../'
alias .4='cd ../../../../'
alias .5='cd ../../../../..'

#open widely used directories
alias apps="cd ~/apps"
alias sites="cd ~/sites"
alias shopify="cd ~/sites/_shopify"
alias sketch="sketchtool"
alias drive="cd ~/'Google Drive File Stream'/'My Drive/'"

# Active Projects
alias whosdustin.com="cd ~/sites/_jekyll/whosdustin.com"
alias vuemastery.com="cd ~/sites/vuemastery.com"

# Servers
alias "pgup"="pg_ctl -D /usr/local/var/postgres start"
